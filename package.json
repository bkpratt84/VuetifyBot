{
  "name": "vuetifybot",
  "version": "1.0.0",
  "description": "A chat bot for the Vuetify Discord channel.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "scale-down": "now scale bot.vuetifyjs.com 0 --team=vuetifyjs --token=$NOW_TOKEN",
    "deploy": "now --team=vuetifyjs --token=$NOW_TOKEN --npm",
    "alias": "now alias --team=vuetifyjs --token=$NOW_TOKEN",
    "scale-up": "now scale bot.vuetifyjs.com 1 1 --team=vuetifyjs --token=$NOW_TOKEN",
    "unalias": "now rm vuetifybot --team=vuetifyjs --safe --yes --token=$NOW_TOKEN"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkpratt84/VuetifyBot.git"
  },
  "author": "",
  "credit": [
    "Forked from: https://github.com/An-Idiots-Guide/guidebot.git"
  ],
  "license": "MIT",
  "dependencies": {
    "discord.js": "^11.2.1",
    "enmap": "^0.4.1",
    "express": "^4.16.2",
    "moment": "^2.18.1",
    "moment-duration-format": "^2.0.1",
    "now-env": "^3.0.2"
  }
}
